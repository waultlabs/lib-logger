{
  "name": "@waultlabs/lib-logger",
  "version": "1.0.1",
  "description": "A federated logger for both local development and AWS",
  "keywords": [
    "logger",
    "aws",
    "cloudwatch",
    "docker",
    "fargate"
  ],
  "author": {
    "name": "FranÃ§ois MASCLEF"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/waultlabs/lib-logger.git"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "build": "npm run mrproper && npm run build:esm && npm run build:cjs && npm run build:types",
    "build:esm": "esbuild ts/index.ts --bundle --platform=node --outdir=\"dist\" --format=esm --target=node22 --minify --out-extension:.js=.mjs",
    "build:cjs": "esbuild ts/index.ts --bundle --platform=node --outdir=\"dist\" --format=cjs --target=node22 --minify --out-extension:.js=.cjs",
    "build:types": "cp ts/index.d.ts dist/index.d.ts",
    "lint": "eslint ./ts",
    "mrproper": "rm -Rf dist",
    "prepare": "husky",
    "test": "vitest run --coverage",
    "test:watch": "vitest"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "19.8.1",
    "@eslint/eslintrc": "3.3.1",
    "@types/node": "24.3.0",
    "@typescript-eslint/eslint-plugin": "8.40.0",
    "@vitest/coverage-v8": "3.2.4",
    "commitlint": "19.8.1",
    "esbuild": "0.25.9",
    "esbuild-plugin-tsc": "0.5.0",
    "husky": "9.1.7",
    "prettier": "3.6.2",
    "ts-node": "10.9.2",
    "typescript": "5.9.2",
    "vitest": "3.2.4"
  }
}
